<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="safetysuitezclaimemployer.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="masterPage" title="Master Page">
		<content>
			<List items="{/employerdata}">
				<items>
					<StandardListItem icon="sap-icon://employee" title="{inquiryfor}" description="{bday}" info="{fname}" type="Navigation" press="createemp"/>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>