<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="safetysuitezclaimemployer.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="masterPage" title="{i18n>masterPageTitle}">
		<content>
			<List mode="SingleSelectMaster" selectionChange="clickClaimBtn" items="{/employerdata}">
				<items>
					<StandardListItem icon="sap-icon://employee" title="{inquiryfor}" description="{bday}" info="{fname}"/>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>