<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="safetysuitezclaimemployer.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="masterPage" title="{i18n>masterPageTitle}">
		<content>
			<List id="list" itemPress="clickClaimBtn" items="{/IncidentsSet}">
				<items>
					<ObjectListItem id="objlist" title="{FamilyName}" type="Active" press="onListItemPress" number="{ManagerPernr}">
						<ObjectAttribute text="{BodypartDes}"/>
						<ObjectAttribute
							text="{path:'IDate', type:'sap.ui.model.type.DateTime', formatOptions: { source: { pattern: 'yyyy-MM-ddTHH:mm:ss.msZ' }, pattern: 'MMM dd,yyyy ' }}"/>
					</ObjectListItem>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>